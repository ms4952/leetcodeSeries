   "source": [
    "strs=[\"flower\",\"flow\",\"flight\"]\n",
    "shortest=min(strs,key=len)\n",
    "p=\"\"\n",
    "for i,c in enumerate(shortest):\n",
    "    f=1\n",
    "    for x in strs:\n",
    "        if c!=x[i]:\n",
    "            f=0\n",
    "            break\n",
    "    if f==0:\n",
    "        break\n",
    "    if f==1:\n",
    "        p=p+c\n",
    "print(p)  \n"
